generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Article {
  id           String    @id @default(uuid()) @db.Uuid
  source_name  String?
  author       String?
  title        String
  description  String?
  urlToImage   String?
  url          String
  content      String?
  published_at DateTime?
  created_at   DateTime  @default(now())
  is_synced    Boolean   @default(false)
  synced_at    DateTime?
}
